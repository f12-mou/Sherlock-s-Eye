<style>
a:link { text-decoration: none; }
a:visited { text-decoration: none; }
a:hover { text-decoration: underline; }
a:active { text-decoration: underline; }
</style>

<% if (casesResult.length>0){%>
<div class="table-responsive">
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Case_ID</th>
        <th scope="col">Case Title</th>
        <th scope="col">Status Code</th>
        <th scope="col">More Details</th>
      </tr>
    </thead>
    <tbody>
        <% for (const demo of casesResult) { %> 
          <tr>
          <td>Sherlocks_Riddle_<%= demo.CASE_ID %></td>
          <td><%= demo.TITLE%></td>
          <td><%= demo.TYPE %></td>
          <td><a href="/api/case/detailscase/<%= demo.CASE_ID%>" target="_blank">View Details</a></td>

          </tr>
          <%}%>
    </tbody>
    </table>
  </div>

  <% }else{%>
    <p>NO CASES IN DATABASE TO SHOW</p>
    <%}%>

      
